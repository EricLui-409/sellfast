<h1> Marketplace </h1>

<table class="table">
  <thead>
      <tr>
          <th scope="col"></th>
          <th scope="col">Item Name</th>
          <th scope="col">Item Description</th>
          <th scope="col">Price to Beat</th>
      </tr>
  </thead>
  <tbody>
    <% for item in @items do%>
    <tr>
      <td><%= item.name %></td>
      <td><%= item.description %></td>
      <td><%= item.current_price %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Add new item', items_path %>